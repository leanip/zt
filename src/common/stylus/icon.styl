@font-face
  font-family: 'zt-icon';
  src:
    url('../fonts/zt-icon.ttf?dbrvtt') format('truetype'),
    url('../fonts/zt-icon.woff?dbrvtt') format('woff'),
    url('../fonts/zt-icon.svg?dbrvtt#zt-icon') format('svg');
  font-weight: normal;
  font-style: normal;


[class^="icon-"], [class*=" icon-"]
  /* use !important to prevent issues with browser extensions that change fonts */
  font-family: 'zt-icon' !important;
  speak: none;
  font-style: normal;
  font-weight: normal;
  font-variant: normal;
  text-transform: none;
  line-height: 1;

  /* Better Font Rendering =========== */
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;


.icon-home:before
  content: "\e901";

.icon-cart:before
  content: "\e93a";

.icon-user:before
  content: "\e971";

.icon-search:before
  content: "\e986";

.icon-list:before
  content: "\e9ba";

